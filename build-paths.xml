<?xml version="1.0"?>
<project>
	<description>
		Common classpaths shared across all submodules.
	</description>

	<path id="compile.path">
	</path>

	<path id="compile.out">
		<path refid="compile.path" />
		<pathelement location="core/target/classes/main" />
	</path>

	<path id="test.compile.path">
		<path refid="compile.out" />
		<pathelement location="libs/junit/v4.8.2/junit-4.8.2.jar" />
	</path>

	<path id="test.run.path">
		<path refid="test.compile.path" />
		<pathelement location="core/target/classes/test" />
	</path>
</project>
